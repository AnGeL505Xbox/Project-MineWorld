            "arrays": { "textures": { "Array.skin": [ "Texture.black", "Texture.white", "Texture.canel" ] } },
            "geometry": "Geometry.default",
            "materials": [ { "*": "Material.default" } ],
            "part_visibility": [ { "tame*": "query.is_tamed" } ],
            "textures": [ "Array.skin[query.variant]" ]
        },
        "controller.render.chihuahua_eye": {
            "arrays": { "textures": { "Array.eye": [ "Texture.brown", "Texture.green" ] } },
            "geometry": "Geometry.default",
            "materials": [ { "*": "Material.default" } ],
            "part_visibility": [ { "tame*": "query.is_tamed" } ],
            "textures": [ "Array.eye[query.mark_variant]" ]
        }
    }
}