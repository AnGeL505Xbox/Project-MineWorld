{
    "format_version": "1.16.100",
    "minecraft:block": {
        "description": {
            "identifier": "mineworld:quantum_planks",
            "properties": {
                "mineworld:playerStep": [ true, false ]
            }
        },
        "components": {
            "minecraft:loot": "loot_tables/blocks/quantumPlanks.json",
            "minecraft:destroy_time": 1.5,
            "minecraft:explosion_resistance": 15,
            "minecraft:block_light_emission": 0.5,
            "minecraft:map_color": "#dae2eb",
            "minecraft:on_step_on": {
                "condition": "q.get_nearby_entities('minecraft:player') == true",
                "event": "playerStep",
				"target": "self"
	  		},
            "minecraft:display_name": "block.mineworld:quantumPlanks"
        },
        "permutations": [
            {
                "condition": "q.get_nearby_entities('minecraft:player') == true",
                "components": {
                    "minecraft:breathability": "solid"
                }
            },
            {
                "condition": "q.get_nearby_entities('minecraft:player') == false",
                "components": {
                    "minecraft:breathability": "air"
                }
            }
        ]
    }
}