    "Mineworld:templates": [
        {
            "version": 1,
            "identifier": "mineworld:amphibia",
            "components": {
                //#region Base
                "minecraft:nameable": {},
                "minecraft:breathable": {
                    "total_supply": 15,
                    "suffocate_time": 0,
                    "breathes_water": true,
                    "breathes_air": true,
                    "generates_bubbles": false
                },
                "minecraft:despawn": { "despawn_from_distance": { "max_distance": 128, "min_distance": 32 } },
                "minecraft:pushable": { "is_pushable": true, "is_pushable_by_piston": true },
                "minecraft:physics": {},
                //#endregion
                //#region Damage
                "minecraft:damage_sensor": {
                    "triggers": {
                        "cause": "lightning",
                        "deals_damage": true,
                        "damage_multiplier": 2000
                    }
                },
                "minecraft:hurt_on_condition": {
                    "damage_conditions": [
                        {
                            "filters": {
                                "test": "in_lava",
                                "subject": "self",
                                "operator": "==",
                                "value": true
                            },
                            "cause": "lava",
                            "damage_per_tick": 4
                        }
                    ]
                },
                //#endregion
                //#region Movement
                "minecraft:movement": { "value": 0.15 },
                "minecraft:underwater_movement": { "value": 0.15 },
                "minecraft:navigation.generic": {
                    "is_amphibious": true,
                    "can_path_over_water": false,
                    "can_swim": true,
                    "can_walk": true,
                    "can_sink": false,
                    "avoid_damage_blocks": true
                },
                "minecraft:movement.amphibious": { "max_turn": 10 },
                "minecraft:jump.static": {},
                //#endregion
                //#region Behavior
                "minecraft:behavior.move_to_water": {
                    "priority": 3,
                    "search_range": 16, "search_height": 5,
                    "search_count": 1, "goal_radius": 0.1
                }, 
                "minecraft:behavior.random_swim": {
                    "priority": 5,
                    "interval": 0,
                    "speed_multiplier": 1,
                    "xz_dist": 30, "y_dist": 15
                },
                "minecraft:behavior.swim_idle": { "priority": 5 },
                "minecraft:behavior.random_look_around": {
                    "priority": 5,
                    "look_time": [ 3, 5 ]
                },
                "minecraft:behavior.move_to_land": {
                    "priority": 6,
                    "search_range": 16, "search_height": 5, "goal_radius": 0.5
                }
                //#endregion
            }
        },
