{
   "format_version": "1.10.0",
   "minecraft:client_entity": {
      "description": {
         "identifier": "mineworld:shadow_bird",
         "materials": { "default": "skeleton" },
         "textures": { "default": "textures/entity/skull_bird" },
         "geometry": { "default": "geometry.skull_bird" },
         "animations": {
            "look_at_target": "animation.common.look_at_target",
            "fly": "animation.skull_bird.fly",
            "walk": "animation.skull_bird.walk",
            "baby": "animation.skull_bird.baby"
         },
         "scripts": {
            "animate": [
               { "walk": "query.is_on_ground" },
               { "fly": "!query.is_on_ground" },
               "look_at_target"
            ]
         },
         "render_controllers": [ "controller.render.base" ],
         "spawn_egg": {
            "base_color": "#242424",
            "overlay_color": "#aaaaaa"
         }
      }
   }
}
